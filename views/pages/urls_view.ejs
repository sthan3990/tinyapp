<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/_header') %>
    <link href="/styles/index.css" rel="stylesheet" type="text/css">
    <title>TinyApp | View</title>
</head>

<body>

  <div class="row">

    <div class="main-wrapper">

      <div class="row">

        <div class="section-wrapper">

          <div class="nav-wrapper">
            <%- include('../partials/_navMember') %>
          </div>

          <div class="form-content">

            <div class="form-items">

              <div class="form-title">

                <p class="login-style">Welcome: <%= username %>

              </div>

              <table class="table table-dark table-sm">


                <thead>
                  <tr>
                    <th>Short URL</th>
                    <th>Long URL</th>
                    <th>Created by</th>
                  </tr>
                </thead>

                <tbody>

                  <td>
                    <%=  shortURL %>
                  </td>

                  <td>
                    <%=  longURL %>
                  </td>

                  <td>
                    <%=  username %>
                  </td>

                </tbody>


              </table>

            </div>
          </div>
          <%- include('../partials/_footer') %>

        </div>
      </div>
    </div>

  </div>

</body>

</html>